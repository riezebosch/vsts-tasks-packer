{
  "name": "vsts-packer-tasks",
  "version": "0.0.1",
  "description": "Packer Tasks for VSTS",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:prod": "npm i && npm run build && npm prune --production && npm dedupe",
    "pretest": "npm run build",
    "test": "mocha --recursive",
    "upload": "npm run build:prod && tfx build tasks upload --task-path ."
  },
  "keywords": [
    "packer",
    "vsts"
  ],
  "author": "Manuel Riezebosch",
  "license": "ISC",
  "dependencies": {
    "fs": "0.0.1-security",
    "azure-pipelines-task-lib": "^2.9.3"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^3.1.0",
    "@stryker-mutator/mocha-framework": "^3.1.0",
    "@stryker-mutator/mocha-runner": "^3.1.0",
    "@stryker-mutator/typescript": "^3.1.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "sinon": "^9.0.2",
    "typescript": "^3.8.3"
  }
}
