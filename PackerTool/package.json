{
  "name": "vsts-packer-tasks",
  "version": "0.0.1",
  "description": "Packer Tasks for VSTS",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:prod": "npm i && npm run build && npm prune --production && npm dedupe --production",
    "pretest": "npm run build",
    "test": "mocha --recursive",
    "upload": "npm run build:prod && tfx build tasks upload --task-path .",
    "postinstall": "patch-package"
  },
  "keywords": [
    "packer",
    "vsts"
  ],
  "author": "Manuel Riezebosch",
  "license": "ISC",
  "dependencies": {
    "azure-pipelines-tool-lib": "^2.0.10",
    "cross-fetch": "^4.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.6.2",
    "@types/node-fetch": "^2",
    "chai": "^6.2.0",
    "mocha": "^11.7.4",
    "patch-package": "^8.0.1",
    "typescript": "^5.9.3"
  }
}
